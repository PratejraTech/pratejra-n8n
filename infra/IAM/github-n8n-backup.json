{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowGitBackupWrite",
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:GetObject"
        ],
        "Resource": "arn:aws:s3:::pratejra-git-backups-prod/*"
      },
      {
        "Sid": "AllowGitBackupList",
        "Effect": "Allow",
        "Action": "s3:ListBucket",
        "Resource": "arn:aws:s3:::pratejra-git-backups-prod"
      }
    ]
  }